{
    "name": "dafiti/web-api-extension",
    "type": "behat-extension",
    "description": "Web API extension for Behat",
    "keywords": ["behat", "api", "test"],
    "homepage": "http://extensions.behat.org",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/Behat/WebApiExtension/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "behat/behat": "~3.3",
        "guzzlehttp/guzzle": "~6",
        "phpunit/phpunit": "6.0.10"
    },
    "require-dev": {
        "symfony/process": "~2.8",
        "silex/silex": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Behat\\WebApiExtension\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
